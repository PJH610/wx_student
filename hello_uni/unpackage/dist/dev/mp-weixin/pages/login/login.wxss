.login {
  width: 750rpx;
}
.login .login_log {
    margin-top: 80px;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    /*x轴对齐方式*/
}
.login .login_log image {
      width: 150rpx;
      height: 150rpx;
}
.login .login_btn {
    margin-top: 50px;
}
.login .login_btn button {
      width: 650rpx;
}
.login .login_text {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    /*x轴对齐方式*/
    margin-top: 25px;
    color: #3f3f3f;
    font-size: 32rpx;
}
.login .login_text text:nth-child(2) {
      margin: 0 8px;
}
.login .agreement {
    width: 750rpx;
    position: fixed;
    bottom: 15px;
    left: 50%;
    margin-left: -300rpx;
}
.login .agreement text {
      font-size: 28rpx;
      color: #777777;
}
.login .agreement .text_color {
      color: #8ec7f7;
}

